package questao10;

import javax.swing.JOptionPane;

public class Questao10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numero1;
		int resultado;
		numero1 = Integer.parseInt(JOptionPane.showInputDialog(null, "Informe o numero a ser fatorado: "));
		if (numero1 > 1) {
			resultado=numero1;
			while (numero1>1) {
				resultado = resultado * (numero1-1);
				numero1--;
				System.out.println(resultado);
			}
			System.out.println("Fatorial  Ã© : " + resultado);
		}
		
	}

}
