package questao10;

import javax.swing.JOptionPane;

public class Questao10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numero=Integer.parseInt(JOptionPane.showInputDialog(null,"Informe o numero a fatorar"));
		System.out.println("O fatorial de 6 Ã© :" + fatorial(numero));
	}
	public static int fatorial(int numero1) {
		int fatorial=1;
		for (int i=numero1;i>=2;i--) {
			fatorial *=i;
		}
		return fatorial;
	}

}
